[include]
    path=~/.gitconfig-local

[alias]
	st = status
	br = branch
	cm = "commit -m"
	co = checkout
  unstage = "reset HEAD"
	adda = "add --all"
	cma = commit -am
	dif = diff --name-only
	pom = push origin master
	diffc = diff --cached
	cmend = commit --amend
	last = log -1
[color]
	ui = auto
[core]
	editor = vim
	excludesfile = ~/dotfiles/gitignore-global
[user]
