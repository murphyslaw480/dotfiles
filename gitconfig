[include]
    path=~/.gitconfig-local

[alias]
	st = status
	br = branch
	cm = "commit -m"
	co = checkout
	rem = "remote -v"
	adda = "add --all"
	cma = commit -am
	dif = diff --name-only
	pom = push origin master
	diffc = diff --cached
	cmend = commit --amend
[color]
	ui = auto
[core]
	editor = vim
	excludesfile = ~/dotfiles/gitignore-global
