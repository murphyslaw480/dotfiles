[alias]
	st      = status
	br      = branch
	ci      = commit
	ca      = "commit -a"
	cm      = "commit -m"
	cma     = "commit -am"
	cmend   = "commit --amend"
	cmendr  = "commit --amend --reuse-message=HEAD"
	co      = checkout
  unstage = "reset HEAD"
	adda    = "add --all"
	dif     = diff --name-only
	pom     = push origin master
	diffc   = diff --cached
	logm    = log master..HEAD
	last    = log -1
  graph   = log --graph --decorate=full --oneline --all
[color]
	ui = auto
[core]
	editor       = vim
	excludesfile = ~/dotfiles/gitignore-global
[user]
	name  = Ryan
	email = ryan@rcorre.net
[push]
	default = upstream
