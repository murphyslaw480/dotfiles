"ponymix inc 5 --max 150 && ~/dotfiles/dwmstatus"
    XF86AudioRaiseVolume
"ponymix dec 5 --max 150 && ~/dotfiles/dwmstatus"
    XF86AudioLowerVolume
"ponymix toggle && ~/dotfiles/dwmstatus"
    XF86AudioMute
"xbacklight -inc 10"
    XF86MonBrightnessUp
"xbacklight -dec 10"
    XF86MonBrightnessDown
